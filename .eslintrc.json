{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "rest-spread-spacing": ["error", "never"],
      "camelcase": "off",
      "react/jsx-props-no-spreading": "off",
      "react/jsx-curly-brace-presence": "off"
    }
}
